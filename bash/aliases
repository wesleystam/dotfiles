alias reload_bash="echo reloading shell; . ~/.bash_profile"
alias reload!="echo restarting app; touch tmp/restart.txt"
alias pow="echo restarting pow; touch ~/.pow/restart.txt"
alias s="cd .."

alias vi="mvim -v"

alias gd="cd vendor/plugins/gaia"
alias sd="cd ../../../"

alias hosts="vi /etc/hosts"
